{
  "schema": {
    "title": "blog",
    "type": "object",
    "properties": {
      "posts": {
        "type": "function",
        "output": {
          "type": "array",
          "items": { "$ref": "#/$defs/Post" }
        },
        "policy": 0,
        "runtime": 0
      }
    },
    "required": ["posts"],
    "$defs": {
      "Post": {
        "type": "object",
        "properties": {
          "id": { "type": "string", "format": "uuid" },
          "title": { "type": "string" },
          "content": { "type": "string" },
          "author": { "$ref": "#/$defs/User" },
          "published": { "type": "boolean" }
        },
        "required": ["id", "title", "content", "author", "published"]
      },
      "User": {
        "type": "object",
        "properties": {
          "id": { "type": "string", "format": "uuid" },
          "name": { "type": "string" },
          "email": { "type": "string", "format": "email" },
          "website": { "type": "string", "format": "uri" }
        },
        "required": ["id", "name", "email"]
      }
    }
  },
  "materializers": [],
  "runtimes": [],
  "policies": []
}
